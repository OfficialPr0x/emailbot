{
  "name": "myg-instagram-bot",
  "version": "1.0.0",
  "description": "Automated Gmail and Instagram account creation bot with AI-powered form filling",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node src/test.js",
    "test:gmail": "node src/test.js gmail",
    "test:full": "node src/test.js full",
    "example:full": "node examples/create-account.js",
    "example:gmail": "node examples/create-gmail-only.js",
    "example:instagram": "node examples/create-instagram-with-existing-gmail.js",
    "check": "node check-setup.js",
    "setup": "npm install && cd frontend && npm install && cd .. && npx playwright install chromium",
    "setup:db": "npx prisma generate && npx prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "frontend:install": "cd frontend && npm install",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "website:install": "cd website && npm install",
    "website:dev": "cd website && npm run dev",
    "website:build": "cd website && npm run build",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "instagram",
    "gmail",
    "automation",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.2",
    "imap": "^0.8.19",
    "mailparser": "^3.6.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.0",
    "playwright": "^1.40.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.33.0",
    "socket.io": "^4.6.0",
    "socks-proxy-agent": "^8.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^6.18.0"
  }
}
